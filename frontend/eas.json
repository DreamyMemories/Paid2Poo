{
    "cli": {
        "appVersionSource": "remote",
        "version": ">= 5.9.1"
    },
    "build": {
        "preview": {
            "android": {
                "buildType": "apk"
            },
            "channel": "preview"
        },
        "development": {
            "android": {
                "buildType": "apk"
            },
            "channel": "development",
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview2": {
            "android": {
                "gradleCommand": ":app:assembleRelease"
            },
            "channel": "preview2"
        },
        "preview3": {
            "developmentClient": true,
            "channel": "preview3"
        },
        "preview4": {
            "distribution": "internal",
            "channel": "preview4"
        },
        "production-apk": {
            "android": {
                "buildType": "apk"
            },
            "channel": "production",
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true,
            "channel": "production"
        }
    },
    "submit": {
        "production": {}
    }
}